{
  "name": "minigame-std",
  "description": "Mini Game Standard Development Library.",
  "author": "jiang115jie@gmail.com",
  "license": "GPL-3.0",
  "version": "1.0.1",
  "source": "src/std/mod.ts",
  "main": "dist/main.cjs",
  "module": "dist/main.mjs",
  "types": "dist/types.d.ts",
  "files": [
    "LICENSE",
    "README.md",
    "package.json",
    "src",
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "check": "pnpm exec tsc --noEmit",
    "lint": "pnpm exec eslint src/",
    "prebuild": "pnpm exec rimraf dist && pnpm run check && pnpm run lint",
    "build": "pnpm exec rollup --config rollup.config.mjs",
    "test": "bun test --preload ./tests/setup.ts",
    "prepublishOnly": "pnpm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JiangJie/minigame-std.git"
  },
  "keywords": [
    "minigame",
    "mina",
    "std",
    "encode",
    "decode",
    "fetch",
    "request",
    "fs",
    "event",
    "onerror",
    "unhandledrejection",
    "socket",
    "base64",
    "storage",
    "clipboard"
  ],
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.13.0",
    "eslint": "^8.57.0",
    "rimraf": "^5.0.7",
    "rollup": "^4.18.0",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-esbuild": "^6.1.1",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@happy-ts/fetch-t": "^1.0.9",
    "@std/path": "npm:@jsr/std__path@^0.224.0",
    "happy-opfs": "^1.0.10",
    "happy-rusty": "^1.1.0",
    "minigame-api-typings": "^3.8.4"
  },
  "packageManager": "pnpm@9.3.0"
}
