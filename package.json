{
  "name": "minigame-std",
  "description": "Mini Game Standard Development Library.",
  "author": "jiang115jie@gmail.com",
  "license": "GPL-3.0",
  "version": "1.0.0",
  "source": "./src/std/mod.ts",
  "main": "./dist/main.cjs",
  "module": "./dist/main.mjs",
  "types": "./dist/types.d.ts",
  "exports": "./src/std/mod.ts",
  "files": [
    "LICENSE",
    "README.md",
    "README.cn.md",
    "package.json",
    "src",
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "check": "npx tsc --noEmit",
