  "repository": {
    "type": "git",
    "url": "git+https://github.com/JiangJie/minigame-std.git"
  },
  "keywords": [
    "minigame",
    "mina",
    "std",
    "encode",
    "decode",
    "fetch",
    "request",
    "fs",
    "event",
    "onerror",
    "unhandledrejection",
    "socket",
    "base64",
    "storage",
    "clipboard"
  ],
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "eslint": "^8.57.0",
    "rimraf": "^5.0.7",
    "rollup": "^4.17.2",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-esbuild": "^6.1.1",
    "typescript": "^5.4.5"
  },
