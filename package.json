{
  "name": "minigame-std",
  "description": "Mini Game Standard Development Library.",
  "author": "jiang115jie@gmail.com",
  "license": "MIT",
  "version": "1.10.0",
  "type": "module",
  "source": "src/std/mod.ts",
  "main": "dist/main.cjs",
  "module": "dist/main.mjs",
  "types": "dist/types.d.ts",
  "files": [
    "LICENSE",
    "README.md",
    "README.en.md",
    "CHANGELOG.md",
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "check": "pnpm exec tsc --noEmit",
    "lint": "pnpm exec eslint .",
    "prebuild": "pnpm dlx rimraf dist && pnpm run check && pnpm run lint",
    "build": "pnpm exec rollup --config rollup.config.mjs",
    "predocs": "pnpm dlx rimraf docs",
    "docs": "pnpm exec typedoc",
    "prepublishOnly": "pnpm run build",
    "playwright:install": "pnpm exec playwright install",
    "test": "pnpm exec vitest run --coverage",
    "test:watch": "pnpm exec vitest watch",
    "test:ui": "pnpm exec vitest --ui"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JiangJie/minigame-std.git"
  },
  "keywords": [
    "minigame",
    "mina",
    "std",
    "encode",
    "decode",
    "fetch",
    "request",
    "fs",
    "event",
    "onerror",
    "unhandledrejection",
    "socket",
    "base64",
    "storage",
    "clipboard",
    "network",
    "crypto",
    "md5",
    "sha",
    "rsa",
    "random",
    "uuid",
    "webaudio",
    "performance"
  ],
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@stylistic/eslint-plugin": "^5.6.1",
    "@vitest/browser-playwright": "^4.0.16",
    "@vitest/coverage-v8": "^4.0.16",
    "@vitest/ui": "^4.0.16",
    "eslint": "^9.39.2",
    "playwright": "^1.57.0",
    "rollup": "^4.54.0",
    "rollup-plugin-dts": "^6.3.0",
    "rollup-plugin-esbuild": "^6.2.1",
    "typedoc": "^0.28.15",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.0",
    "vitest": "^4.0.16"
  },
  "dependencies": {
    "@happy-ts/fetch-t": "^1.4.0",
    "@std/path": "npm:@jsr/std__path@^1.1.3",
    "fflate": "^0.8.2",
    "happy-opfs": "^1.11.0",
    "happy-rusty": "^1.6.1",
    "minigame-api-typings": "^3.8.14",
    "rsa-oaep-encryption": "^1.1.0",
    "tiny-future": "^1.2.0",
    "tiny-invariant": "^1.3.3"
  }
}
